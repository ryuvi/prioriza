{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*",
      ],
      "@src/*": [
        "./src/*",
      ],
      "@components/*": [
        "./components/*",
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@stores/*": [
        "./stores/*"
      ],
      "@constants/*": [
        "./constants/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
